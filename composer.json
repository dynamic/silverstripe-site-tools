{
    "name": "dynamic/silverstripe-site-tools",
    "description": "Tools to build common functionality in SilverStripe sites.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "require": {
        "dynamic/silverstripe-linkable": "^1.0",
        "silvershop/silverstripe-hasonefield": "^2.1",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "silverstripe/vendor-plugin": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.0",
        "unclecheese/display-logic": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\SiteTools\\": "src/",
            "Dynamic\\SiteTools\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
